<idea-plugin>
    <extensions defaultExtensionNs="com.intellij">
        <completion.contributor language="kotlin" implementationClass="dev.jbang.idea.completion.directive.JBangKotlinCompletionContributor"/>
        <completion.contributor language="kotlin" implementationClass="dev.jbang.idea.completion.sources.JBangKotlinSourcesCompletionContributor"/>
        <completion.contributor language="kotlin" implementationClass="dev.jbang.idea.completion.deps.JBangKotlinDepsGavCompletionContributor"/>
        <runLineMarkerContributor language="kotlin" implementationClass="dev.jbang.idea.run.JBangRunLineMarkerContributor"/>
        <annotator language="kotlin" implementationClass="dev.jbang.idea.highlight.JBangDirectiveHighlighterAnnotator"/>
    </extensions>
</idea-plugin>